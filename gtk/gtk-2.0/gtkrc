        {
                function                = FLAT_BOX
                detail                  = "entry_bg"
                state                   = NORMAL
                overlay_file		= "images/entry/oo-entry-bg.svg"
	        overlay_border		= { 0, 0, 0, 0 }
                overlay_stretch         = TRUE
        }
