        {
                function                = FLAT_BOX
                detail                  = "entry_bg"
                state                   = ACTIVE
                overlay_file		= "images/entry/oo-entry-bg.svg"
	        overlay_border		= { 0, 0, 0, 0 }
                overlay_stretch         = TRUE
        }
	image
	{
		function		= SHADOW
		detail			= "entry"
                state                   = NORMAL
		shadow			= IN
		file			= "images/entry/oo-entry.svg"
		border			= { 3,3, 4, 4 }
		stretch			= TRUE
	}
