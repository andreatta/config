		font_name 		= "* Bold 9"
}

# Chromium#############################################################
style "chrome-gtk-frame"
{
	ChromeGtkFrame::frame-color	= "#121212"
    ChromeGtkFrame::inactive-frame-color = "#2a2a2a"
    ChromeGtkFrame::incognito-frame-color = mix(0.4, @selected_bg_color, "#121212")
    ChromeGtkFrame::incognito-inactive-frame-color = mix (0.4, @selected_bg_color, "#222222")

    ChromeGtkFrame::frame-gradient-size		= 18
    ChromeGtkFrame::frame-gradient-color		= "#424242"
    ChromeGtkFrame::inactive-frame-gradient-color	= "#525252"

    ChromeGtkFrame::incognito-frame-gradient-size		= 18
    ChromeGtkFrame::incognito-frame-gradient-color		= "#363636"
    ChromeGtkFrame::incognito-inactive-frame-gradient-color	= mix (0.4, @selected_bg_color, "#363636")

    ChromeGtkFrame::scrollbar-trough-color 	= "#1f1f1f"
    ChromeGtkFrame::scrollbar-slider-normal-color 		= "#424242"
    ChromeGtkFrame::scrollbar-slider-prelight-color 	= "mix(0.2, "#424242", @selected_bg_color)"

}

# Chromium Menu Item###################################################
style "chrome_menu_item"
{
        bg[SELECTED] 		= @selected_bg_color
}

# Xfce Headers#########################################################
style "xfce_header"  =  "gtk_color"
{
        text[NORMAL] 		= shade(1.1, @fg_color)
        base[NORMAL] 		= "#0e0e0e"
}

# Text Style###########################################################
style "text"   
{
		base[NORMAL] 		= "#e2e2e2"
		
engine "murrine"
 	{
		text_shade 	= 0.7
		textstyle 	= 2
	}
}  

# Text Style Notebook##################################################
style "notebook-text"  =  "gtk_color"
{
		#fg[INSENSITIVE]  		=  "#2c2e2f"
		#text[INSENSITIVE]  	=  "#2c2e2f"
	
engine "murrine"
 	{
		text_shade 	= 0.6
		textstyle 	= 2
	}
}

# XChat Text Input#####################################################
style "xchat_text"  
{
		base[NORMAL] 		= "#030303"
}
# Null Style###########################################################
style "null"
{
	engine "pixmap"
	{
	image
	{
	    function	= BOX
	    file	= "images/other/null.png"
	    stretch	= TRUE
        }
 	}
}

